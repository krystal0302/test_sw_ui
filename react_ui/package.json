{
  "name": "react-ui",
  "description": "UI powered by React framework",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "concurrently --prefix-colors 'yellow,blue' --kill-others 'npm run mock:api' 'npm run dev'",
    "build": "vite build",
    "preview": "vite preview",
    "deploy:copy": "concurrently --prefix-colors 'yellow,blue' 'cp ./dist/wrapper.html ../server/public/' 'cp -R ./dist/assets ../server/public/'",
    "deploy": "npm run build && npm run deploy:copy",
    "mock:api": "json-server -p 5001 --watch ./data/swarm.json",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook"
  },
  "author": {
    "name": "John Wu <john.wu@farobottech.com>"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@material-ui/icons": "^4.11.3",
    "@monaco-editor/react": "^4.4.6",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "axios": "^1.2.0",
    "babel-plugin-macros": "^3.1.0",
    "dotenv": "^16.0.3",
    "monaco-themes": "^0.4.3",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^4.6.3",
    "react-folder-tree": "^5.1.1",
    "react-icons": "^4.7.1",
    "react-loading-overlay": "^1.0.1",
    "react-multi-carousel": "^2.8.2",
    "react-query": "^3.39.2",
    "react-reflex": "^4.0.9",
    "react-router-dom": "^6.4.3",
    "react-select": "^5.6.1",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.1",
    "react-visibility-sensor": "^5.1.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.3",
    "styled-components": "^5.3.6",
    "toastify-js": "^1.12.0",
    "ua-parser-js": "^1.0.32",
    "url-loader": "^4.1.1"
